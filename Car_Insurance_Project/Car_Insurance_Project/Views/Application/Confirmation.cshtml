@model IEnumerable<Car_Insurance_Project.Models.Application>

@{
    ViewBag.Title = "Confirmation";
}

<h2>Confirmation</h2>

<table class="table">
    <tr>
        <th>
            First Name
        </th>
        <th>
            Last Name
        </th>
        <th>
            Age
        </th>
        <th>
            Email Address
        </th>
        <th>
            Car Year
        </th>
        <th>
            Make
        </th>
        <th>
            Model
        </th>
        <th>
            Dui
        </th>
        <th>
            Tickets
        </th>
        <th>
            Coverage
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.Reverse().Take(1))
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.FirstName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.LastName)
    </td>
    <td>
        @Html.DisplayFor(model => item.Age)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.EmailAddress)
    </td>
    <td>
        @Html.DisplayFor(model => item.Car_Year)
    </td>
    <td>
        @Html.DisplayFor(model => item.Make)
    </td>
    <td>
        @Html.DisplayFor(model => item.Model)
    </td>
    <td>
        @Html.DisplayFor(model => item.Dui)
    </td>
    <td>
        @Html.DisplayFor(model => item.Tickets)
    </td>
    <td>
        @Html.DisplayFor(model => item.Coverage)
    </td>
    <td>
        @Html.ActionLink("Generate Quote", "Quote", new { id = item.Id })
    </td>
</tr>
    }

</table>
