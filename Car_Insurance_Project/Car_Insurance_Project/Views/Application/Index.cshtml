@model IEnumerable<Car_Insurance_Project.Models.Application>

@{
    ViewBag.Title = "Admin";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Add New Applicant", "Create")
</p>
<p>
    @Html.ActionLink("Admin View", "Admin")
</p>
<table class="table">
    <tr>
        <th>
            First Name
        </th>
        <th>
            Last Name
        </th>
        <th>
            Age
        </th>
        <th>
            Email Address
        </th>
        <th>
            Car Year
        </th>
        <th>
            Make
        </th>
        <th>
            Model
        </th>
        <th>
            Dui
        </th>
        <th>
            Tickets
        </th>
        <th>
            Coverage
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Quote)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.FirstName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.LastName)
    </td>
    <td>
        @Html.DisplayFor(model => item.Age)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.EmailAddress)
    </td>
    <td>
        @Html.DisplayFor(model => item.Car_Year)
    </td>
    <td>
        @Html.DisplayFor(model => item.Make)
    </td>
    <td>
        @Html.DisplayFor(model => item.Model)
    </td>
    <td>
        @Html.DisplayFor(model => item.Dui)
    </td>
    <td>
        @Html.DisplayFor(model => item.Tickets)
    </td>
    <td>
        @Html.DisplayFor(model => item.Coverage)
    </td>
    <td>
        $@Html.DisplayFor(modelItem => item.Quote)
    </td>
    <td>
        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
    </td>
</tr>
}

</table>
